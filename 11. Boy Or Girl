#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
cin>>s;
    sort(s.begin(),s.end());
    int l,ans=0;
    l=s.length();
    for(int i=1;i<l;i++)
    {
        if(s[i-1]!=s[i])
        ans++;
        
    }
    if(ans&1)
    cout<<"CHAT WITH HER!"<<endl;
    else
    cout<<"IGNORE HIM!"<<endl;
}
